<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <properties resource="Lecture/Evaluation/conf/jdbc.properties"></properties>

  <typeAliases>
    <typeAlias type="Lecture.Evaluation.evaluation" alias="evaluation"/>
    <typeAlias type="Lecture.Evaluation.likey" alias="likey"/>
    <typeAlias type="Lecture.Evaluation.user" alias="user"/>
    
  </typeAliases>

  <environments default="development">
    <environment id="development">
      <transactionManager type="JDBC"/>
      <dataSource type="POOLED">
        <property name="driver" value="${jdbc.driver}"/>
        <property name="url" value="${jdbc.url}"/>
        <property name="username" value="${jdbc.username}"/>
        <property name="password" value="${jdbc.password}"/>
      </dataSource>
    </environment>
  </environments>
  
  <mappers>
    <mapper resource="Lecture/Evaluation/mapper/EvaluationMapper.xml"/>
    <mapper resource="Lecture/Evaluation/mapper/UserMapper.xml"/>
    <mapper resource="Lecture/Evaluation/mapper/LikeyMapper.xml"/>
    <mapper resource="Lecture/Evaluation/mapper/BoardMapper.xml"/>
    
  </mappers>
</configuration>

